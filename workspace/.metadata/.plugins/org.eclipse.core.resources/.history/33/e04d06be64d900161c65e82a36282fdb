package petStore;

public class DogList {
	
	private Animal[] dList = new Animal[6];
	private int index = 0;
	
	public void add(Animal d){
		if(index < dList.length)
		{
			dList[index]=d;
			System.out.println("Animal added at index" + index);
			index++;
		}
		
		else{
			System.out.println("The list is full already");
			
		}
	}

}
